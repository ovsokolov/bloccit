<div class="row">
  <div class="col-md-12">
    <h1>All Question</h1>
    <span><%= link_to "New", new_question_path, class: 'btn btn-success' %></span>
  </div>
</div>
<% @questions.each do |question| %>
<div class="media">
  <div class="media-body">
    <h4 class="media-heading">
      <%= link_to question.title, question %>
    </h4>
  </div>
</div>
<% end %>
